        <table class="table table-dark">
          <thead>
            <tr>
              <th scope="col">Day</th>
              <th scope="col">Body Weight(kg)</th>
              <th scope="col">Body Fat Ratio(%)</th>
              <th scope="col"></th>
            </tr>
          </thead>
          <tbody>
<% @physical_data.each do |data|%>
            <tr>
              <td><%= data.date %></td>
              <td><%= data.body_weight %></td>
              <td><%= data.body_fat_ratio %></td>
              <% if current_user == data.user %>
                <td><%= link_to "Edit", edit_physical_path(data.id), class: 'btn btn-success btn-sm' %>     <%= link_to "Delete", data, method: :delete, data: { confirm: "You sure?" }, class: 'btn btn-danger btn-sm' %></td>
              <% else %>
                <td></td>
              <% end %>
            </tr>
<% end %>
          </tbody>
        </table>